Algoritmo ejercicio_1
	// Digites las variables de entrada 
	Definir a , b , c Como Entero;
	// Digite el valor de las variables
	a <-3 ;
	b <-7 ;
	// luego los operadores indicados
	c <-2*a+b-a mod 3 ;
	// luego muestre como saliada el resultado
	Escribir 'Dado a = ',a,' y b = ',b,' el valor de c = 2*',a,'+',b,'-',a,'mod 3 es:',c ;
	
FinAlgoritmo
